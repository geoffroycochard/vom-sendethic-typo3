                    </settings.template>
                    <settings.successPage>
                        <TCEforms>
                            <label>Page de succès</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </settings.successPage>
                    <settings.errorPage>
                        <TCEforms>
                            <label>Page d'erreur</label>
